<tests xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:mftf:Test/etc/testSchema.xsd">
    <test name="AdminContactDisableEmailTest">
        <annotations>
            <title value="Admin Config Test"/>
            <stories value="Show that Admin config to disable Email saved  "/>
            <description value="Verify that Admin config to disable Email saved successfully  "/>
            <features value="admin"/>
            <severity value="MAJOR"/>
            <testCaseId value="AdminLoginPage-02"/>
            <group value="adminContactDisableEmail"/>
        </annotations>
        <before>
            <actionGroup ref="AdminLoginActionGroup" stepKey="loginAsAdmin"/>
        </before>
        <actionGroup ref="AdminContactDisableEmailActionGroup" stepKey="AdminDisableEmailConfiguration">
        </actionGroup>
    </test>
</tests>
