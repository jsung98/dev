<sections xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:mftf:Page/etc/SectionObject.xsd">
    <section name="AdminContactConfigurationSection">
        <element name="clickOnStore" type="button" selector="//li[@data-ui-id='menu-magento-backend-stores']//span" timeout="1000"/>
        <element name="clickOnConfig" type="button" selector="(//span[contains(text(),'Configuration')])[2]" timeout="1000"/>
        <element name="clickOnContact" type="button" selector="(//span[contains(text(),'Contacts')])" timeout="1000"/>
        <element name="clickOnEnableModule" type="select" selector="//select[@id='contact_contact_enabled']//option[@value='1']" timeout="1000"/>
        <element name="clickOnEnableEmail" type="select" selector="//select[@id='contact_email_enabled']//option[@value='1']" timeout="1000"/>
        <element name="enterEmail" type="input" selector="//input[@id='contact_email_recipient_email']" timeout="1000"/>
        <element name="clickOnSave" type="button" selector="(//span[contains(text(),'Save Config')])[1]" timeout="1000"/>
    </section>
</sections>
