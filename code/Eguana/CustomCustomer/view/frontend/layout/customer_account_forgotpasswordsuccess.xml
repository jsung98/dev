<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="1column" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
        <title>Completed Find Password</title>
    </head>
    <body>
        <referenceBlock name="page.main.title">
            <action method="setPageTitle">
                <argument translate="true" name="title" xsi:type="string">Check your mailbox</argument>
            </action>
        </referenceBlock>
        <referenceContainer name="content">
            <block class="Magento\Framework\View\Element\Template" name="forgotPasswordsuccess" template="Eguana_CustomCustomer::forgotpasswordsuccess.phtml"/>
        </referenceContainer>
        <referenceContainer name="footer" remove="true"/>
    </body>
</page>
